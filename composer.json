{
    "name": "elegantweb/laravel-transactional-events",
    "description": "Transactional events for the Laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sina Sharifzade",
            "email": "sharifzadesina@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elegant\\Events\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegant\\Events\\EventServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
